#include<stdio.h> 
void main()
{
    float number, sum = 0, i;
     scanf("%f", &number);
    for (i = 1; i <= number; i++)
    {
        sum = sum + (1 / i);
        if (i == 1)
            printf("\n 1 +");
        else if (i == number) 
		     printf(" (1 / %f)", i);
        else
            printf(" (1 / %f) + ", i);
    }
    printf("\n %.2f", sum);
}
